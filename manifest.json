{

  "description": "Bypass HikariNoAkari links on Firefox",
  "manifest_version": 2,
  "name": "HikariJUMP",
  "version": "1.0",
  "homepage_url": "https://github.com/raphasanori/HikariJUMP",
  "icons": {
    "48": "icons/border-48.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.hikarinoakariost.info/*"],
      "js": ["hikarijump.js"]
    }
  ]

}
